{
  "name": "@connext/lighthouse",
  "version": "0.2.1-beta.10",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "start:prover": "node --enable-source-maps dist/tasks/prover/index.js",
    "start:process": "node --enable-source-maps dist/tasks/processFromRoot/index.js",
    "start:propagate": "node --enable-source-maps dist/tasks/propagate/index.js",
    "lint": "eslint ./src --ext .ts --env node",
    "test": "nyc ts-mocha --require 'test/globalTestHook.ts' --check-leaks --exit --timeout 60000 'test/**/*.spec.ts'",
    "clean": "rimraf ./dist ./tsconfig.tsBuildInfo",
    "build": "tsc --build ./tsconfig.build.json",
    "verify": "yarn test && yarn clean && yarn build && yarn lint --max-warnings 0",
    "purge": "yarn clean && rimraf ./coverage ./node_modules",
    "tsp": "yarn run tsp:root \"$@\" --cwd $(pwd)",
    "version": "yarn version"
  },
  "author": "Connext",
  "license": "ISC",
  "dependencies": {
    "@arbitrum/sdk": "3.0.0-beta.12",
    "@connext/nxtp-adapters-database": "workspace:*",
    "@connext/nxtp-adapters-relayer": "workspace:*",
    "@connext/nxtp-adapters-subgraph": "workspace:*",
    "@connext/nxtp-txservice": "workspace:*",
    "@connext/nxtp-utils": "workspace:*",
    "@eth-optimism/sdk": "1.6.9",
    "@gelatonetwork/relay-sdk": "^2.1.0",
    "@sinclair/typebox": "0.25.10",
    "dd-trace": "3.8.0",
    "dotenv": "16.0.3",
    "ethers": "5.7.2",
    "fastify": "4.10.2",
    "graphql": "16.6.0",
    "pg": "8.8.0",
    "prom-client": "14.1.0",
    "zapatos": "6.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.14.1",
    "@types/mocha": "10.0.0",
    "@types/pg": "8.6.5",
    "@types/sinon": "10.0.13",
    "concurrently": "7.6.0",
    "eslint": "8.28.0",
    "interval-promise": "1.4.0",
    "mocha": "10.1.0",
    "nodemon": "2.0.20",
    "nyc": "15.1.0",
    "pino-pretty": "9.1.1",
    "rimraf": "3.0.2",
