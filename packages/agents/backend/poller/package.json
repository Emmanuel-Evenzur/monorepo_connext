{
  "name": "@connext/backend-poller",
  "version": "0.2.0-alpha.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "dev": "concurrently -i -k -n 'app,tsc' -c 'green,cyan' 'nodemon'",
    "start": "node --enable-source-maps dist/index.js'",
    "lint": "eslint ./src --ext .ts --env node",
    "test": "nyc ts-mocha --check-leaks --exit --timeout 60000 'test/**/*.spec.ts'",
    "clean": "rimraf ./dist ./tsconfig.tsBuildInfo",
    "build": "tsc --build ./tsconfig.build.json",
    "verify": "yarn test && yarn clean && yarn build && yarn lint --max-warnings 0",
    "purge": "yarn clean && rimraf ./coverage ./node_modules",
    "tsp": "yarn run tsp:root \"$@\" --cwd $(pwd)",
    "zapatos": "zapatos",
    "dbmate": "dbmate"
  },
  "nodemonConfig": {
    "exec": "node --enable-source-maps ./dist/index.js | pino-pretty",
    "delay": "500",
    "ext": "js,json",
    "watch": []
  },
  "eslintIgnore": [
    "graphqlsdk.ts"
  ],
  "author": "Connext",
  "license": "ISC",
  "dependencies": {
    "@connext/nxtp-adapters-subgraph": "workspace:*",
    "@connext/nxtp-utils": "workspace:*",
    "@sinclair/typebox": "0.23.5",
    "dotenv": "16.0.1",
    "ethers": "5.6.6",
    "interval-promise": "1.4.0",
    "pg": "8.7.3",
    "zapatos": "5.0.4"
  },
  "devDependencies": {
    "@types/pg": "8.6.5",
    "concurrently": "7.2.0",
    "eslint": "8.15.0",
    "nodemon": "2.0.16",
    "nyc": "15.1.0",
    "pg-mem": "2.4.3",
    "pino-pretty": "7.6.1",
    "rimraf": "3.0.2",
    "sinclair": "0.2.0",
    "sinon": "14.0.0",
    "ts-mocha": "10.0.0",
    "ts-node": "10.7.0",
    "tslib": "2.4.0",
    "typescript": "4.6.4"
  },
  "gitHead": "937a7cde93e6ac1e151c6374f48adf83d3fa4ec6",
  "stableVersion": "0.2.0-alpha.0"
}
